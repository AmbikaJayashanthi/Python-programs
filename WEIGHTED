n=int(input("Enter the number of events:"))
S=[]
E=[]
W=[]
weight=0
for i in range(n):
    s=int(input("Starting time:"))
    S.append(s)
    e=int(input("Ending time:"))
    E.append(e)
    w=int(input("Weight:"))
    W.append(w)
i=1
j=0
while i<n:
    if S[i]<=E[j]:
        weight=weight+W[j] 
    j=j+1  
    i=i+1
weight=weight+W[j]   
print(weight)
            
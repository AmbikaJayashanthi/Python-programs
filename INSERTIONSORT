def insertion(elements):
    n=len(elements)
    for i in range(1,n):
        key=elements[i]
        j=i-1
        while j>=0 and elements[j]>key:
            elements[j+1]=elements[j]
            j=j-1
        elements[j+1]=key
    return elements

elements=[10,12,2,5,7,100,87]
res=insertion(elements)
print(res)